<VirtualHost *:80>
   DocumentRoot "/home/otavio/Projetos/seci/"
   ServerName seci.sic-maracanau.com.br

   # This should be omitted in the production environment
   SetEnv APPLICATION_ENV development

   <Directory "/home/otavio/Projetos/seci/">
       Options Indexes MultiViews FollowSymLinks
       #AllowOverride All
       #Order allow,deny
       #Allow from all
         Options -Indexes 
        AllowOverride All
        Require all granted
   </Directory>

#RewriteEngine on
#RewriteCond %{SERVER_NAME} =seci.sic-maracanau.com.br
#RewriteRule ^ https://%{SERVER_NAME}%{REQUEST_URI} [END,NE,R=permanent]
</VirtualHost>

